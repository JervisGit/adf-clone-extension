{
  "name": "adf-pipeline-clone",
  "displayName": "ADF Pipeline Clone",
  "description": "A visual pipeline editor for Azure Data Factory and Synapse Analytics",
  "version": "0.0.1",
  "publisher": "local",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/adf-pipeline-clone"
  },
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "adf-pipeline-clone.openPipeline",
        "title": "ADF: Open Pipeline Editor",
        "category": "Pipeline"
      },
      {
        "command": "adf-pipeline-clone.addActivity",
        "title": "ADF: Add Activity",
        "category": "Pipeline"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run lint && node esbuild.js",
    "watch": "node esbuild.js --watch",
    "package": "npm run lint && node esbuild.js --production",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2"
  }
}
