      "partitionLowerBoundQuery": { "type": "text", "label": "Partition lower bound", "placeholder": "Lower bound value", "conditional": { "field": "partitionOptionQuery", "value": "DynamicRange" }, "nestedConditional": { "field": "useQuery", "value": "Query" } },
