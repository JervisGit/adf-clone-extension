      "partitionColumnName": { "type": "text", "label": "Partition column name", "placeholder": "Column name", "conditional": { "field": "partitionOption", "value": "DynamicRange" }, "nestedConditional": { "field": "useQuery", "value": "Table" } },
      "partitionUpperBound": { "type": "text", "label": "Partition upper bound", "placeholder": "Upper bound value", "conditional": { "field": "partitionOption", "value": "DynamicRange" }, "nestedConditional": { "field": "useQuery", "value": "Table" } },
      "partitionLowerBound": { "type": "text", "label": "Partition lower bound", "placeholder": "Lower bound value", "conditional": { "field": "partitionOption", "value": "DynamicRange" }, "nestedConditional": { "field": "useQuery", "value": "Table" } },
      "partitionOptionQuery": { "type": "radio", "label": "Partition option", "options": ["None", "Dynamic range"], "optionValues": ["None", "DynamicRange"], "default": "None", "conditional": { "field": "useQuery", "value": "Query" } },
      "partitionColumnNameQuery": { "type": "text", "label": "Partition column name", "placeholder": "Column name", "conditional": { "field": "partitionOptionQuery", "value": "DynamicRange" }, "nestedConditional": { "field": "useQuery", "value": "Query" } },
